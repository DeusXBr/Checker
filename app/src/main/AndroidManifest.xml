<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="checker.ifrs.edu.checker">

    <application
        android:name=".app.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.activities.AvaliacaoActivity" />
        <activity android:name=".view.activities.ListaAvaliacaoActivity" />
        <activity android:name=".view.activities.SobreActivity" />
        <activity android:name=".view.activities.PerguntasActivity"></activity>
    </application>

</manifest>