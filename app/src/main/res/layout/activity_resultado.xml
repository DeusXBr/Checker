<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"

    android:id="@+id/activity_resultado"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="checker.ifrs.edu.checker.view.activities.ResultadoActivity"
    >

    <include
        android:id="@+id/layout.activityresultado.toolbar"
        layout="@layout/toolbar"
        />

    <include
        android:id="@+id/layout.activityresultado.content_resultado"
        layout="@layout/content_resultado"
        />

    <RelativeLayout
        android:id="@+id/rl_footer"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true" >

        <include
            android:id="@+id/layout.activityresultado.footer"
            layout="@layout/footer"
            />
    </RelativeLayout>

</RelativeLayout>
